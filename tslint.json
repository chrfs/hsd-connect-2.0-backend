{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "variable-name": false,
    "ter-prefer-arrow-callback": false,
    "import-name": false,
    "only-arrow-functions": false,
    "prefer-array-literal": false,
    "no-bitwise": false
  }
}
